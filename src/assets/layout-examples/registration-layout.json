{
    "name": "first-form",
    "elements": [
        {
            "name": "headerTemplate",
            "type": "header",
            "value": "Runner Information",
            "classMap": {
                "heading-1": true
            }
        },
        {
            "name": "name",
            "type": "text",
            "placeholder": "Name",
            "validation": [
                {
                    "type": "required",
                    "ranking": 1
                }
            ],
            "size": 1
        },
        {
            "name": "email",
            "type": "email",
            "required": true,
            "placeholder": "Email Address",
            "validation": [
                {
                    "type": "email",
                    "ranking": 1
                }
            ],
            "size": 1
        },
        {
            "name": "address",
            "type": "text",
            "placeholder": "Address",
            "validation": [
                {
                    "type": "required",
                    "ranking": 1
                }
            ],
            "size": 1
        },
        {
            "name": "city",
            "type": "text",
            "placeholder": "City",
            "validation": [
                {
                    "type": "required",
                    "ranking": 1
                }
            ],
            "size": 1
        },
        {
            "name": "state",
            "type": "text",
            "placeholder": "State",
            "validation": [
                {
                    "type": "required",
                    "ranking": 1
                }
            ],
            "size": 1
        },
        {
            "name": "zip",
            "type": "text",
            "placeholder": "Zip",
            "validation": [
                {
                    "type": "required",
                    "ranking": 1
                }
            ],
            "size": 1
        },
        {
            "name": "phone",
            "type": "text",
            "placeholder": "Phone",
            "validation": [
                {
                    "type": "required",
                    "ranking": 1
                }
            ],
            "size": 1
        },
        {
            "name": "date",
            "type": "datepicker",
            "placeholder": "Date of birth",
            "required": true,
            "dateConfig": {
                "useToggleSuffix": true
            },
            "validation": [
                {
                    "type": "required",
                    "ranking": 1
                },
                {
                    "type": "date",
                    "ranking": 1
                }
            ],
            "size": 1
        },
        {
            "name": "tShirtSize",
            "type": "select",
            "placeholder": "T-Shirt Size",
            "validation": [
                {
                    "type": "required",
                    "ranking": 1
                }
            ],
            "selectConfig": {
                "valueField": "id",
                "visibleProps": [
                    "title"
                ],
                "optionsList": [
                    {
                        "title": "Small",
                        "id": "s"
                    },
                    {
                        "title": "Medium",
                        "id": "m"
                    },
                    {
                        "title": "Large",
                        "id": "l"
                    },
                    {
                        "title": "Extra large",
                        "id": "xl"
                    }
                ]
            },
            "size": 5
        },
        {
            "name": "genderLabel",
            "type": "label",
            "value": "Gender",
            "size": 1
        },
        {
            "name": "gender",
            "required": true,
            "type": "radiogroup",
            "placeholder": "Gender",
            "radioElements": [
                {
                    "label": "Male",
                    "value": "m"
                },
                {
                    "label": "Female",
                    "value": "f"
                }
            ],
            "size": 4
        },
        {
            "name": "RaceLabel",
            "type": "label",
            "value": "Chose Race",
            "size": 1
        },
        {
            "name": "race",
            "required": true,
            "type": "radiogroup",
            "radioElements": [
                {
                    "label": "5k - $25",
                    "value": "smallCache"
                },
                {
                    "label": "10K - $25",
                    "value": "aLotOfCache"
                }
            ],
            "size": 7
        }
    ],
    "layout": [
        {
            "lines": [
                [
                    "headerTemplate"
                ],
                [
                    "name",
                    "email"
                ],
                [
                    "address",
                    "city"
                ],
                [
                    "state",
                    "zip"
                ],
                [
                    "phone",
                    "date"
                ],
                [
                    "tShirtSize",
                    "genderLabel",
                    "gender"
                ],
                [
                    "RaceLabel",
                    "race"
                ]
            ]
        }
    ]
}