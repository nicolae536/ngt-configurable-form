{
    "name": "first-form",
    "groupElements": [
        {
            "class": "first-row",
            "wrapInExpansionPane": false,
            "addCardClass": true,
            "title": "Personal information",
            "description": "",
            "elements": [
                {
                    "elementsOnLine": [
                        {
                            "name": "name",
                            "required": true,
                            "type": "text",
                            "placeholder": "First Name",
                            "size": 1
                        },
                        {
                            "name": "lastName",
                            "required": true,
                            "type": "text",
                            "placeholder": "Last name",
                            "size": 1
                        },
                        {
                            "name": "uniqueIdentifier",
                            "required": true,
                            "type": "number",
                            "placeholder": "Unique code",
                            "size": 2
                        }
                    ]
                },
                {
                    "elementsOnLine": [
                        {
                            "name": "description",
                            "type": "textarea",
                            "placeholder": "Short description",
                            "size": 1
                        }
                    ]
                }
            ]
        },
        {
            "wrapInExpansionPane": true,
            "title": "Address",
            "description": "Your adress",
            "elements": [
                {
                    "elementsOnLine": [
                        {
                            "name": "selectCity",
                            "required": true,
                            "type": "select",
                            "placeholder": "City",
                            "selectConfig": {
                                "valueField": "cityId",
                                "visibleProps": [
                                    "cityName"
                                ]
                            },
                            "size": 1
                        },
                        {
                            "name": "street",
                            "required": true,
                            "type": "text",
                            "placeholder": "Address",
                            "size": 2
                        },
                        {
                            "name": "streetNumber",
                            "required": true,
                            "type": "number",
                            "placeholder": "Number",
                            "size": 1
                        }
                    ]
                },
                {
                    "elementsOnLine": [
                        {
                            "name": "postCode",
                            "required": true,
                            "type": "number",
                            "placeholder": "Post code",
                            "configurationChangeMap": {
                                "defaultConfig": {
                                    "hidden": true,
                                    "size": 0
                                },
                                "configurationChange": [
                                    {
                                        "linkedElement": "selectCity",
                                        "expectedValue": 0,
                                        "configurationChange": {
                                            "hidden": true,
                                            "size": 0
                                        }
                                    },
                                    {
                                        "linkedElement": "selectCity",
                                        "expectedValue": 1,
                                        "configurationChange": {
                                            "type": "textarea",
                                            "hidden": false,
                                            "size": 1
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            ]
        },
        {
            "wrapInExpansionPane": true,
            "title": "Delivery",
            "description": "Delivery location",
            "elements": [
                {
                    "elementsOnLine": [
                        {
                            "name": "deliveryStreet",
                            "required": true,
                            "type": "text",
                            "placeholder": "Address",
                            "size": 2
                        },
                        {
                            "name": "deliveryNumber",
                            "required": true,
                            "type": "number",
                            "placeholder": "Number",
                            "size": 1
                        },
                        {
                            "name": "deliveryPostCode",
                            "required": true,
                            "type": "number",
                            "placeholder": "Post code",
                            "size": 1
                        }
                    ]
                }
            ]
        }
    ]
}