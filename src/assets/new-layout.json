{
    "name": "first-form",
    "elements": [
        {
            "name": "name",
            "type": "text",
            "placeholder": "First Name",
            "validation": [
                {
                    "type": "required",
                    "ranking": 1
                },
                {
                    "type": "minLength",
                    "staticMetadata": 3,
                    "ranking": 2
                }
            ],
            "size": 1
        },
        {
            "name": "lastName",
            "type": "text",
            "placeholder": "Last name",
            "validation": [
                {
                    "type": "required",
                    "ranking": 1
                }
            ],
            "size": 1
        },
        {
            "name": "email",
            "type": "email",
            "required": true,
            "placeholder": "Email Address",
            "validation": [
                {
                    "type": "email",
                    "ranking": 1
                }
            ],
            "size": 1
        },
        {
            "name": "phone",
            "type": "tel",
            "placeholder": "Phone",
            "required": true,
            "validation": [
                {
                    "type": "phone",
                    "ranking": 1
                }
            ],
            "size": 1
        },
        {
            "name": "dateOfBirth",
            "type": "datepicker",
            "placeholder": "Date of birth",
            "required": true,
            "dateConfig": {
                "useToggleSuffix": true
            },
            "validation": [
                {
                    "type": "date",
                    "ranking": 1
                }
            ],
            "size": 1
        },
        {
            "name": "address",
            "type": "text",
            "placeholder": "Address",
            "required": true,
            "validation": [
                {
                    "type": "minLength",
                    "staticMetadata": 4,
                    "ranking": 1
                }
            ],
            "size": 1
        },
        {
            "name": "city",
            "type": "autocomplete",
            "placeholder": "City",
            "selectConfig": {
                "valueField": "city",
                "visibleProps": [
                    "city",
                    "population"
                ]
            },
            "required": true,
            "size": 1
        },
        {
            "name": "state",
            "type": "text",
            "placeholder": "State",
            "required": true,
            "size": 1
        },
        {
            "name": "detailiatedAddress",
            "type": "slidetoggle",
            "placeholder": "Detaliated address",
            "size": 1
        },
        {
            "name": "street",
            "required": true,
            "type": "text",
            "placeholder": "Street"
        },
        {
            "name": "streetNumber",
            "required": true,
            "type": "number",
            "placeholder": "Street number"
        },
        {
            "name": "zip",
            "type": "text",
            "placeholder": "Zip",
            "required": true,
            "size": 1
        },
        {
            "name": "startDt",
            "type": "datepicker",
            "placeholder": "Start date",
            "required": true,
            "dateConfig": {
                "useToggleSuffix": true
            },
            "validation": [
                {
                    "type": "maxDate",
                    "staticMetadata": "endDate",
                    "ranking": 1
                }
            ],
            "size": 1
        },
        {
            "name": "endDate",
            "type": "datepicker",
            "required": true,
            "dateConfig": {
                "useToggleSuffix": true
            },
            "validation": [
                {
                    "type": "minDate",
                    "staticMetadata": "startDt",
                    "ranking": 1
                }
            ],
            "placeholder": "End date",
            "size": 1
        },
        {
            "name": "expDescription",
            "type": "textarea",
            "placeholder": "Short experience description"
        },
        {
            "name": "tShirtSize",
            "type": "select",
            "placeholder": "T-Shirt Size",
            "required": true,
            "selectConfig": {
                "valueField": "id",
                "visibleProps": [
                    "title"
                ],
                "optionsList": [
                    {
                        "title": "Small",
                        "id": "s"
                    },
                    {
                        "title": "Medium",
                        "id": "m"
                    },
                    {
                        "title": "Large",
                        "id": "l"
                    },
                    {
                        "title": "Extra large",
                        "id": "xl"
                    }
                ]
            },
            "size": 5
        },
        {
            "name": "genderLabel",
            "type": "label",
            "value": "Gender :",
            "size": 1
        },
        {
            "name": "gender",
            "required": true,
            "type": "radiogroup",
            "placeholder": "Gender",
            "radioElements": [
                {
                    "label": "Male",
                    "value": "m"
                },
                {
                    "label": "Female",
                    "value": "f"
                }
            ],
            "size": 4
        },
        {
            "name": "RaceLabel",
            "type": "label",
            "value": "Chose Race :",
            "size": 1
        },
        {
            "name": "race",
            "required": true,
            "type": "radiogroup",
            "radioElements": [
                {
                    "label": "5k - $25",
                    "value": "smallCache"
                },
                {
                    "label": "10K - $25",
                    "value": "aLotOfCache"
                }
            ],
            "size": 7
        }
    ],
    "groupElements": [
        {
            "type": "ngt-card",
            "name": "runnerInformation",
            "title": "Runner Information"
        },
        {
            "type": "mat-expansion-pane",
            "name": "address",
            "title": "Address",
            "description": "Current address"
        },
        {
            "type": "mat-expansion-pane",
            "name": "runningExperience",
            "title": "Running experience",
            "description": "The number of years which you perform running"
        },
        {
            "type": "ngt-card",
            "name": "contestRegistration",
            "title": "Contest registration",
            "description": "Current contest details"
        }
    ],
    "layout": [
        {
            "group": "runnerInformation",
            "lines": [
                [
                    "name",
                    "lastName",
                    "email"
                ],
                [
                    "phone",
                    "dateOfBirth"
                ]
            ]
        },
        {
            "group": "address",
            "lines": [
                [
                    "address",
                    "city",
                    "state"
                ],
                [
                    "detailiatedAddress"
                ],
                [
                    "street",
                    "number",
                    "zip"
                ]
            ]
        },
        {
            "group": "runningExperience",
            "lines": [
                [
                    "startDt",
                    "endDate"
                ],
                [
                    "expDescription"
                ]
            ]
        },
        {
            "group": "contestRegistration",
            "lines": [
                [
                    "tShirtSize",
                    "genderLabel",
                    "gender"
                ],
                [
                    "RaceLabel",
                    "race"
                ]
            ]
        }
    ],
    "linkDefinitions": [
        {
            "name": "street",
            "defaultConfig": {
                "hidden": true,
                "size": 0
            },
            "configurationChange": [
                {
                    "linkedElement": "detailiatedAddress",
                    "expectedValue": true,
                    "configurationChange": {
                        "hidden": false,
                        "size": 1
                    }
                }
            ]
        },
        {
            "name": "streetNumber",
            "defaultConfig": {
                "hidden": true,
                "size": 0
            },
            "configurationChange": [
                {
                    "linkedElement": "detailiatedAddress",
                    "expectedValue": true,
                    "configurationChange": {
                        "hidden": false,
                        "size": 1
                    }
                }
            ]
        },
        {
            "name": "zip",
            "defaultConfig": {
                "hidden": true,
                "size": 0
            },
            "configurationChange": [
                {
                    "linkedElement": "detailiatedAddress",
                    "expectedValue": true,
                    "configurationChange": {
                        "hidden": false,
                        "size": 1
                    }
                }
            ]
        }
    ]
}